<div id="tableContainer" class="tableContainer">
    <table class="scrollTable" width="100%" cellspacing="0" cellpadding="0" border="0">
        <thead class="fixedHeader">
            <tr class="alternateRow">
                <th width="20">
                    <%= check_box_tag 'selectall' %>
                </th>
                <th>
                    Processes
                </th>
                <th>
                </th>
                <th>
                    Tools
                </th>
            </tr>
        </thead>
        <tbody class="scrollContent">
            <% @processes.each do |proc| -%>
            <tr class="<%= cycle('normalRow','alternateRow') %>">
                <td width="20">
                    <%= check_box_tag 'hq_proc_ids[]', proc.id %>
                </td>
                <td>
                    <%= proc.name %>
                </td>
                <td>
                </td>
                <td>
                </td>
            </tr>
            <% end -%>
        </tbody>
    </table>
</div>
<%= submit_to_remote 'rsrcs_by_procs', 'Filter', :url => { :action => 'rsrcs_by_procs' } %>